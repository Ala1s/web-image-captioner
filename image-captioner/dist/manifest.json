{
  "name": "Image Captioner",
  "version": "0.1.0",
  "description": "TensorFlow.js chrome extension for captioning images on the web",
    "icons":{
  	"128":"128x128.png",
  	"32": "32x32.png",
  	"16": "16x16.png"
  },
  "homepage_url": "https://github.com/Ala1s",
  "author": "Alexandra Babicheva",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
